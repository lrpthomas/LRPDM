  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "knex migrate:latest --knexfile knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "db:seed": "knex seed:run --knexfile knexfile.ts",
    "web:dev": "webpack serve --config apps/web-app/webpack.config.js --mode development",
    "web:build": "webpack --config apps/web-app/webpack.config.js --mode production",
    "web:serve": "webpack serve --config apps/web-app/webpack.config.js --mode development --port 3001",
    "install:all": "pnpm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit --coverage",
    "test:spatial:validation": "PGPASSWORD=$POSTGRES_PASSWORD jest tests/spatial-validation.test.ts --forceExit",
    "test:spatial:performance": "PGPASSWORD=$POSTGRES_PASSWORD jest tests/spatial-performance.test.ts --forceExit",
    "test:performance": "ts-node src/scripts/spatial-performance-audit.ts",
    "security:audit": "pnpm audit --audit-level moderate",
    "security:scan": "echo 'Security scan placeholder - integrate with Snyk/CodeQL'",
    "lint": "echo 'Linting placeholder - add ESLint configuration'",
    "type-check": "tsc --noEmit"
