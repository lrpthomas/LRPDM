{
  "name": "@lrpdm/api-legacy",
  "version": "1.0.0",
  "description": "Legacy Express API for LRPDM GIS Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "knex migrate:latest --knexfile ../../knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile ../../knexfile.ts",
    "db:seed": "knex seed:run --knexfile ../../knexfile.ts"
  },
  "keywords": [
    "gis",
    "express",
    "api",
    "postgis"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mapbox/togeojson": "^0.16.2",
    "@xmldom/xmldom": "^0.9.8",
    "adm-zip": "^0.5.16",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "fast-xml-parser": "^5.2.5",
    "geojson-validation": "^1.0.2",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "knex": "^3.1.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.1",
    "node-stream-zip": "^1.15.0",
    "pg": "^8.16.3",
    "postgis": "^1.0.5",
    "reproject": "^1.2.7",
    "shapefile": "^0.6.6",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.3",
    "@types/adm-zip": "^0.5.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.3",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.15.34",
    "@types/pg": "^8.15.4",
    "@types/uuid": "^10.0.0",
    "jest": "^30.0.3",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=22.11.0"
  }
}